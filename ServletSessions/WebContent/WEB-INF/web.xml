<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ServletSessions</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <!-- CreateSession -->
	<servlet>
		<servlet-name>obj1</servlet-name>
		<servlet-class>sessions.CreateSession</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>obj1</servlet-name>
		<url-pattern>/session</url-pattern>
	</servlet-mapping>
	
	<!-- DisplaySession -->
	<servlet>
		<servlet-name>obj2</servlet-name>
		<servlet-class>sessions.DisplaySession</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>obj2</servlet-name>
		<url-pattern>/displaysession</url-pattern>
	</servlet-mapping>
	
</web-app>